#include <iostream>
#include <sstream>
using namespace std;

void reverse(int *mas,int first,int last){
    if(last<first) exit(0);
    int length=last - first;
    for (int i  = 0; i < length / 2; i++)
        swap(mas[first+i], mas[last - i - 1]);
}

int main() {
    unsigned int length =0;
    
    string str;
    getline(cin,str);
    istringstream stream1(str);
    if(!(stream1>>length)){
        cout<<"An error has occuried while reading input data."<<endl;
        exit(0);
    }
    
    int *mas=new int [length];
    int el;
    string mas_str;
    getline(cin,mas_str);
    istringstream stream (mas_str);
    for(int i=0;i<length;i++){
        if(!(stream>>mas[i])){
            cout<<"An error has occuried while reading input data."<<endl;
            exit(0);
        }
    }
    if(stream>>el){
        cout<<"An error has occuried while reading input data."<<endl;
        exit(0);
    }
    
    unsigned int razdel =0;
    
    string str2;
    getline(cin,str2);
    istringstream stream2(str2);
    if(!(stream2>>razdel)){
        cout<<"An error has occuried while reading input data."<<endl;
        exit(0);
    }
    
    reverse(mas,0,razdel);
    reverse(mas,razdel,length);
    reverse(mas,0,length);
    
    for(int i=0;i<length;i++){
        cout<<mas[i]<<" ";
    }
    cout<<endl;
    delete [] mas;
    return 0;
}
